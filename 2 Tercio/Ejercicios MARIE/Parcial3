Input
			Store    	X
			Load    	X
			Subt    	ONEH
			Skipcond    000
			Jump        endif
			Jump        if
if,    		Load     	X
			Subt    	TEN
			Skipcond    400
			Jump        endif
			Jump        if2
if2,    	Load    	X
			Add        	X
			Store    	Y
			Jump    	endif
endif,    	Load    	Y
			Output
			Halt

X,        	Dec        0
Y,        	Dec        3
ONEH,    	Dec        100
TEN,    	Dec        10